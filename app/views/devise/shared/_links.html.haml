- if controller_name != 'sessions'
  = link_to t('devise.sessions.new.sign_in'), new_session_path(resource_name), class: 'button is-primary'

- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to t('devise.registrations.new.sign_up'), new_registration_path(resource_name), class: 'button'
